<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.NekoMapper">
    <select id="getNeko" resultType="com.example.demo.domain.Neko">
        SELECT * FROM neko_table WHERE id = #{id}
    </select>
    <select id="getNekoList" resultType="com.example.demo.domain.Neko">
        SELECT * FROM neko_table
    </select>
    <insert id="insertNeko" parameterType="com.example.demo.domain.Neko">
        INSERT INTO neko_table (id, name, age) VALUES (#{id}, #{name}, #{age})
    </insert>
</mapper>